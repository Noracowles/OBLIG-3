def my_str_len(data):
    lengths= [] #Lager variabelen legths og putter en tom liste i den
    for word in data: #For alle "ord" i listen data sÃ¥:
        lengths.append(len(word)) #len: lengden pÃ¥ word, append legger den bakerst i listen
    return lengths #return sender lengths tilbake til der vi kalte funksjonen

def my_max(numbers):
    biggest=numbers[0]
    for num in numbers:
        if num > biggest:
            biggest = num
    return biggest

